i=0;
j=0;
answer_link="HI";
cat <<EOF | 
/* Original code Copyright (c) $year_created $author_name[$((++i))]
   Modified code Copyright (c) $year_modified $modifier_name[$((++i))]
   Licensed under the $license_name[$((++i))]

   Original code posted to this question[$((++i))] $(if [ -z "$answer_link" ]; then echo ""; else echo "and answer[$((++i))]"; fi). This comment generated by cite-stackoverflow[$((++i))].

   [$((++j))]: $author_link
   [$((++j))]: $modifier_link
   [$((++j))]: $license_link
   [$((++j))]: $question_link
   [$((++j))]: $answer_link
   [$((++j))]: $cite_stackoverflow_link
 */
EOF
